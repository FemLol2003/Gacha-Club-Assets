      public function presetupdatex() : void
      {
         if(parent.presetclub == 40 && this.openaboutpreset == 1)
         {
            this.aboutpick = presetchoice + (parent.presetpage * 10 - 10);
            this.aboutpreset.gotoAndStop(2);
            this.openaboutpreset = 2;
         }
         else
         {
            parent.colchoice = this.colorchoice;
            this.addpresetnum = parent.presetpage * 10 - 10;
            if(this.swapchoice == 1)
            {
               parent.importchoice = 1;
               parent.presetpick = presetchoice + this.addpresetnum;
               parent.updatepreset();
            }
            else if(this.swapchoice == 2)
            {
               parent.importchoice = 2;
               parent.presetpick = presetchoice + this.addpresetnum;
               parent.updatepreset();
            }
            else if(this.swapchoice == 3)
            {
               parent.importchoice = 3;
               parent.presetpick = presetchoice + this.addpresetnum;
               parent.updatepreset();
            }
            else if(this.swapchoice == 4)
            {
               parent.importchoice = 4;
               parent.presetpick = presetchoice + this.addpresetnum;
               parent.updatepreset();
            }
            if(parent.presetclub == 41)
            {
               if(parent.ylink == "")
               {
                  this.youtubebt.visible = false;
               }
               else
               {
                  this.youtubebt.visible = true;
                  this.youtubebt.namex.text = parent.yname;
                  this.currentlink = parent.ylink;
               }
            }
            this.openaboutpreset = 1;
            parent.botnav.undobt.visible = true;
         }
      }
      
      public function updatepresetpage() : void
      {
         if(parent.presetclub == 1)
         {
            parent.maxpresetpage = 12;
         }
         else if(parent.presetclub >= 2 && parent.presetclub <= 39)
         {
            parent.maxpresetpage = 10;
         }
         else if(parent.presetclub == 40)
         {
            parent.maxpresetpage = 2;
         }
         else if(parent.presetclub == 41)
         {
            parent.maxpresetpage = 16;
         }
         if(parent.presetclub <= 40)
         {
            this.youtubebt.namex.text = "";
            this.youtubebt.visible = false;
            parent.yname = "";
            parent.ylink = "";
            this.currentlink = "";
         }
         if(parent.presetclub >= 2)
         {
            this.allpresets.gotoAndStop(10 + parent.presetpage + parent.presetclub * 10 - 10);
         }
         else
         {
            this.allpresets.gotoAndStop(parent.presetpage + parent.presetclub * 10 - 10);
         }
         this.presetbts.clubx.gotoAndStop(parent.presetclub);
         this.presetbts.pagex.text = "Page " + parent.presetpage + "/" + parent.maxpresetpage;
      }
      
